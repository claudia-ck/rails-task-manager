<h1>New Task</h1>
<%= form_with model: @task do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :details %>
    <%= f.text_field :details %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
<p><%= link_to "Back to list", tasks_path %></p>
